# ble_project/tests/test.config

#
# Tệp chứa dữ liệu mẫu để kiểm thử các thiết bị BLE.
#

[BloodPressureMonitor_TestData]
# Gói tin ACK (xác nhận) cho lệnh handshake
ack_handshake = 06

# Một frame kết quả đo hợp lệ (ví dụ)
# [FC] [Systolic=120] [Diastolic=80] [Pulse=75] [Checksum=...]
result_frame = FC78504B17

# Một frame báo lỗi checksum (Error code 0x03)
error_frame_checksum = FD03

# Một frame báo lỗi lệnh không hợp lệ (Error code 0x01)
error_frame_invalid_cmd = FD01

[WeighingScale_TestData]
# Dữ liệu cân nặng mẫu (ví dụ: 65.5 kg)
weight_frame = AABBCCDD

# Gói tin ACK mà test sẽ kiểm tra xem ứng dụng có gửi đi không
ack_frame = 06